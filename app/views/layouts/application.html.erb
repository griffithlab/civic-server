<!DOCTYPE html>
<html>
<head>
  <title>CIVIC</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% flash.each do |type, message| %>
  <div>
    <p><%= message %></p>
  </div>
<% end %>

<%= yield %>

</body>
</html>
